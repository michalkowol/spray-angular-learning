<configuration>
  <logger name="fr.dudie.nominatim" level="WARN" />
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>[%-5level] [%date{yyyy/MM/dd HH:mm:ss.SSS Z}] [%thread] [%X{akkaSource}] [%logger{0}] - %msg%n</pattern>
		</encoder>
	</appender>
	<root level="DEBUG">
		<appender-ref ref="STDOUT" />
	</root>
</configuration>
